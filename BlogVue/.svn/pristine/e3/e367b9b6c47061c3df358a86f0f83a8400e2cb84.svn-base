.mop-load-div {
    box-pack: center;
    box-align: center;
    height: 100px;
    display: -moz-box;
    -moz-box-pack: center;
    -moz-box-align: center;
    display: -webkit-box;
    -webkit-box-pack: center;
    -webkit-box-align: center;
    -o-box-pack: center;
    -o-box-align: center;
    position: relative;
    z-index: 100;
}

.wave {
    min-width: 60px;
    min-height: 60px;
    width: 60px;
    height: 60px;
}
.wave>div {
    background-color: #56aafa;
    min-height: 60px;
    height: 100%;
    width: 10%;
    margin-left: 10%;
    display: inline-block;
    -webkit-animation: stretchdelay 1.2s infinite ease-in-out;
    -moz-animation: stretchdelay 1.2s infinite ease-in-out;
    -o-animation: stretchdelay 1.2s infinite ease-in-out;
    animation: stretchdelay 1.2s infinite ease-in-out;
}

.wave .rect2 {
    -webkit-animation-delay: -1.1s;
    -moz-animation-delay: -1.1s;
    -o-animation-delay: -1.1s;
    animation-delay: -1.1s;
}

.wave .rect3 {
    -webkit-animation-delay: -1s;
    -moz-animation-delay: -1s;
    -o-animation-delay: -1;
    animation-delay: -1;
}
.wave .rect4 {
    -webkit-animation-delay: -.9s;
    -moz-animation-delay: -.9s;
    -o-animation-delay: -.9s;
    animation-delay: -.9s;
}
.wave .rect5 {
    -webkit-animation-delay: -.8s;
    -moz-animation-delay: -.8s;
    -o-animation-delay: -.8s;
    animation-delay: -.8s;
}


@-webkit-keyframes stretchdelay{0%,40%,100%{-webkit-transform:scaleY( 0.4 )}20%{-webkit-transform:scaleY( 1.0 )}}@-moz-keyframes stretchdelay{0%,40%,100%{-moz-transform:scaleY( 0.4 )}20%{-moz-transform:scaleY( 1.0 )}}@-o-keyframes stretchdelay{0%,40%,100%{-o-transform:scaleY( 0.4 )}20%{-o-transform:scaleY( 1.0 )}}@keyframes stretchdelay{0%,40%,100%{transform:scaleY( 0.4 )}20%{transform:scaleY( 1.0 )}}