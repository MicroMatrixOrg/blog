.path-nav h1 {
	font-size: 16px;
	color: #999;
	margin: auto;
	padding-left: 20px;
	line-height: 56px;
	font-weight: 400;
  }
  .page-wrap {
	position: absolute;
	padding: 0 5px 15px 15px;
	left: 0;
	right: 0;
	bottom: 0;
	background: #fff;
  }
  .clearfix {
	*zoom: 1;
	padding: 15px;
	background: #f7f7f7;
	margin-bottom: 15px;
  }
  .wrap-main .main-box .main-contents {
	background: #fff;
	border: 1px solid #eee;
	border-radius: 4px;
	box-sizing: border-box;
	margin-bottom: 15px;
  }
  .wrap-main .main-box,
  .wrap-main .main-box .main-contents {
	padding: 15px;
	min-width: 760px;
	height: 100%;
	position: relative;
  }
  .wrap-main {
	height: 100%;
  }
  .main-bd {
	position: relative;
	height: 100%;
	overflow: auto;
	background-color: #f8f9fb;
  }
  
  .main_container {
	height: calc(100% - 60px);
  }
  .mt10 {
	margin-top: 10px;
  }
  .layout-bd-right {
	overflow: hidden;
	position: relative;
	height: 100%;
  }
  .layout-main {
	width: 100%;
	height: 100%;
	overflow: auto;
  }
  