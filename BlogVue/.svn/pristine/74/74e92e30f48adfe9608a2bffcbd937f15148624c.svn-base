.ivu-btn-info {
	background: #9e7ae5!important;
	border-color: #9e7ae5!important
}

.ivu-btn-info:active,
.ivu-btn-info:hover {
	background: #ac8ee9!important;
	border-color: #ac8ee9!important
}

.ivu-btn-error {
	color: #f16543;
	border-color: #f16543;
	background: #fff
}

