.clearfix {
	*zoom: 1;
	padding: 15px;
	background: #f7f7f7;
	margin-bottom: 15px;
}

.clearfix:after,
.clearfix:before {
	display: table;
	content: "";
	line-height: 0
}

.ivu-page-item-active{background: #8791a5; border: 1px solid #8791a5;}
.ivu-table-body .ivu-table-cell span{display: -webkit-box;text-overflow: ellipsis;overflow : hidden;text-overflow: ellipsis;-webkit-line-clamp: 2;-webkit-box-orient: vertical;}



.clearfix:after {
	clear: both
}

.wrap-main {
	height: 100%
}

.wrap-main .main-box {
	border-radius: 5px;
	width: 100%;
	height: 100%
}

.wrap-main .main-box,
.wrap-main .main-box .main-contents {
	padding: 15px;
	min-width: 760px;
	min-height: 100%;
	position: relative
}

.wrap-main .main-box .main-contents {
	background: #fff;
	border: 1px solid #eee;
	border-radius: 4px;
	box-sizing: border-box;
	margin-bottom: 15px
}

.ivu-table-wrapper {
	border: none
}

.ivu-table:after,
.ivu-table:before {
	background: none
}

.ivu-table-header th {
	height: 48px;
	color: silver;
	font-size: 12px;
	background: #fff;
	border: none
}

.ivu-table-header th span {
	font-size: 12px
}

.ivu-table td {
	height: 40px;
	border: none;
	border-top: 1px solid #ecedf3
}

.ivu-table td span {
	font-size: 12px
}

.my-border {
	border-top: 1px solid #e9eaec;
	height: 1px
}

.operating .ivu-btn {
	padding: 0;
	border: 0;
	width: 35px
}

.operating .ivu-btn .ivu-icon {
	color: #9f9f9f;
	font-size: 18px;
	width: 30px;
	height: 30px;
	line-height: 30px!important;
	border-radius: 50%
}

.operating .ivu-btn .ivu-icon:hover {
	background: #9e7ae5;
	transition: all .3s ease-in;
	-webkit-transition: all .3s ease-in;
	color: #fff
}

.table-bottom-box {
	padding-bottom: 60px
}

.page-wrap {
	position: absolute;
	padding: 0 5px 15px 15px;
	left: 0;
	right: 0;
	bottom: 0;
	background: #FFFFFF;
}

.page-wrap .ivu-page {
	position: relative
}

.small-page-wrap {
	margin-top: 20px
}

.scree-input {
	width: 300px
}

.scree-input .ivu-input,
